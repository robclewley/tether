</file>

<file src="interface/cursors.png">
  <cursor name="goto" frames="6" x="1" y="1" 
        width="32" height="32" pixelalpha="true">
    <frame anim_frame="0" slot-x="0" slot-y="0"/>
    <frame anim_frame="1" slot-x="0" slot-y="1"/>
    <frame anim_frame="2" slot-x="0" slot-y="2"/>
    <frame anim_frame="3" slot-x="0" slot-y="3"/>
    <frame anim_frame="4" slot-x="0" slot-y="4"/>
    <frame anim_frame="5" slot-x="0" slot-y="5"/>
  </cursor>
  <cursor name="patrol" frames="6" x="1" y="1" 
        width="32" height="32" pixelalpha="true">
    <frame anim_frame="0" slot-x="1" slot-y="0"/>
    <frame anim_frame="1" slot-x="1" slot-y="1"/>
    <frame anim_frame="2" slot-x="1" slot-y="2"/>
    <frame anim_frame="3" slot-x="1" slot-y="3"/>
    <frame anim_frame="4" slot-x="1" slot-y="4"/>
    <frame anim_frame="5" slot-x="1" slot-y="5"/>
  </cursor>
  <cursor name="select" frames="6" x="1" y="1" 
        width="32" height="32" pixelalpha="true">
    <frame anim_frame="0" slot-x="2" slot-y="0"/>
    <frame anim_frame="1" slot-x="2" slot-y="1"/>
    <frame anim_frame="2" slot-x="2" slot-y="2"/>
    <frame anim_frame="3" slot-x="2" slot-y="3"/>
    <frame anim_frame="4" slot-x="2" slot-y="4"/>
    <frame anim_frame="5" slot-x="2" slot-y="5"/>
  </cursor>
  <cursor name="invalid" frames="6" x="1" y="1" 
        width="32" height="32" pixelalpha="true">
    <frame anim_frame="0" slot-x="3" slot-y="0"/>
    <frame anim_frame="1" slot-x="3" slot-y="1"/>
    <frame anim_frame="2" slot-x="3" slot-y="2"/>
    <frame anim_frame="3" slot-x="3" slot-y="3"/>
    <frame anim_frame="4" slot-x="3" slot-y="4"/>
    <frame anim_frame="5" slot-x="3" slot-y="5"/>
  </cursor>
  <cursor name="attack" frames="6" x="1" y="1" 
        width="32" height="32" pixelalpha="true">
    <frame anim_frame="0" slot-x="4" slot-y="0"/>
    <frame anim_frame="1" slot-x="4" slot-y="1"/>
    <frame anim_frame="2" slot-x="4" slot-y="2"/>
    <frame anim_frame="3" slot-x="4" slot-y="3"/>
    <frame anim_frame="4" slot-x="4" slot-y="4"/>
    <frame anim_frame="5" slot-x="4" slot-y="5"/>
  </cursor>
  <cursor name="wait" frames="6" x="1" y="1" 
        width="32" height="32" pixelalpha="true">
    <frame anim_frame="0" slot-x="5" slot-y="0"/>
    <frame anim_frame="1" slot-x="5" slot-y="1"/>
    <frame anim_frame="2" slot-x="5" slot-y="2"/>
    <frame anim_frame="3" slot-x="5" slot-y="3"/>
    <frame anim_frame="4" slot-x="5" slot-y="4"/>
    <frame anim_frame="5" slot-x="5" slot-y="5"/>
  </cursor>

</file>

