<tileset name="MoonPy Default" path="data/graphics" version="1.0"
	 tile_width="24" tile_height="24" edge_tile_height="24"
         panel_width="24" panel_height="24">
 
<file src="terrain/tile_crater.png">
    <terrain name="grass" x="1" y="1" 
             width="24" height="24" pixelsalpha="false">
    <frame slot-x="0" slot-y="0"/>
  </terrain>
</file>

<file src="terrain/tile_water.png">
    <terrain name="water" x="1" y="1" 
             width="24" height="24" pixelsalpha="false">
    <frame slot-x="0" slot-y="0"/>
  </terrain>
</file>


<file src="terrain/crater.png">
  <unit name="crater" frames="1" x="1" y="1" 
        width="48" height="48" pixalalpha="true">
    <frame dir="0" anim_frame="0" slot-x="0" slot-y="0"/>
  </unit>
</file>

<file src="weapons/bomb.png">
  <unit name="bomb" frames="1" x="1" y="1" 
